{
  "name": "aqw-skins-backend",
  "version": "1.0.0",
  "description": "Backend API for AQW Skins Loot Box System",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "node --check src/server.js",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:fairness": "NODE_ENV=test jest --testPathPattern=fairness",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "migrate": "node database/migrate.js",
    "seed": "node database/seed.js",
    "backup": "node database/backup.js"
  },
  "keywords": [
    "aqw",
    "lootbox",
    "fairness",
    "casino",
    "gaming"
  ],
  "author": "AQW Skins Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "morgan": "^1.10.0",
    "winston": "^3.11.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "crypto": "^1.0.1",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.2",
    "stripe": "^14.9.0",
    "node-cron": "^3.0.3",
    "socket.io": "^4.6.0",
    "i18next": "^23.7.8",
    "i18next-http-middleware": "^3.5.0",
    "fingerprintjs": "^3.4.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.11"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
